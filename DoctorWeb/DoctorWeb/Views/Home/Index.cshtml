<script type="text/javascript">
    jQuery("#ReferredBy").change(function () {
        if (jQuery("#ReferredBy").val() == "1") {
            jQuery("#Referred_Doctor").css("display", "block");
            jQuery("#Referred_Relative").css("display", "none");
        }
        else if (jQuery("#ReferredBy").val() == "3") {
            jQuery("#Referred_Relative").css("display", "block");
            jQuery("#Referred_Doctor").css("display", "none");
        }
        else {
            jQuery("#Referred_Doctor").css("display", "none");
            jQuery("#Referred_Relative").css("display", "none");
        }
    })
    jQuery("#red").click(function () {
        jQuery("#step4").css("display", "block");
    });
    jQuery("#green").click(function () {
        jQuery("#step4").css("display", "none");
    });
    jQuery("#yellow").click(function () {
        jQuery("#step4").css("display", "none");
    });

    jQuery('#prostocheck').change(function () {
        jQuery("#prostotext").prop("disabled", !$(this).is(':checked'));
    });
</script>