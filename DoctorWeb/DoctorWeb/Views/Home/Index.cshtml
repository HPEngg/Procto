            <script>
                $(document).ready(function (e) {
                    $('select[name=patient_department]').change(function () {
                        if ($('select[name=patient_department]').val() == 'AYURVED')
                            $("#ayurveda_c_o").show(1);
                        else
                            $("#ayurveda_c_o").css("display", "none");
                        $('input[name=patient_co_aurveda]').val('');
                    });

                    $('select[name=patient_habit_label]').change(function () {
                        if ($('select[name=patient_habit_label]').val() == 'other') {
                            $('input[name=patient_habit]').css("display", "block");
                            $('input[name=patient_habit]').val('');
                        }
                        else {
                            $('input[name=patient_habit]').val('');
                            $('input[name=patient_habit]').css("display", "none");
                            $('input[name=patient_habit]').val($('select[name=patient_habit_label]').val());
                        }

                    });



                    $('select[name=patient_doctor_referl_labal]').change(function () {

                        $('input[name=patient_doctor_referl]').val('');

                        if ($(this).val() == "doctor") {
                            $('input[name=patient_doctor_referl]').css('display', 'block');

                            $('input[name=patient_relative_referl]').css('display', 'none');
                        }
                        else if ($(this).val() == "patient/relative") {
                            $('input[name=patient_relative_referl]').css('display', 'block');
                            $('input[name=patient_doctor_referl]').css('display', 'none');
                        }
                        else {
                            $('input[name=patient_doctor_referl]').hide();
                            $('input[name=patient_relative_referl]').hide();
                        }

                    });

                });

            </script>

